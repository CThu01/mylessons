package com.jdc.mkt.entity;

import java.io.Serializable;

public class Product implements Serializable{

	
	private static final long serialVersionUID = 1L;
	
	private int id;
	private String name;
	private Price price;
	
	public enum Price {
		RETAIL,WHOLESALE;
		private int price;
		
		private Price(int price) {
			this.price = price;
		}
	}
	

}
