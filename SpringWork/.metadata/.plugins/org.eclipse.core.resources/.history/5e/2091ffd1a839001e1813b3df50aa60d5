package com.jdc.mkt.entity;

import java.io.Serializable;
import java.util.Map;

public class Product implements Serializable{

	
	private static final long serialVersionUID = 1L;
	
	private int id;
	private String name;
	private Map<Price, Integer> price;
	
	public enum Price {
		RETAIL,WHOLESALE;
	}
	

}
